[alias]
	amend = commit --amend
	autosquash = rebase -i --autosquash --autostash
	br = branch
	bra = branch -a
	brr = branch -r
	cached = diff --cached
	ci = commit
	co = checkout
	cob = checkout -b
	fixup = "!f() { git commit --fixup=${1:-HEAD}; }; f"
	oneline = log --oneline
	sign-off = commit --amend --signoff
	st = status
[pull]
